{
  "name": "ionic-auth-firebaseui",
  "description": "Ionic UI component for firebase authentication powered by @firebase, @angular and @angular/fire",
  "version": "0.0.1",
  "homepage": "https://github.com/AnthonyNahas/ionic-auth-firebaseui",
  "author": {
    "name": "Anthony Nahas",
    "url": "https://github.com/AnthonyNahas"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/AnthonyNahas/ionic-auth-firebaseui.git"
  },
  "license": "MIT",
  "keywords": [
    "angular",
    "ionic",
    "UI",
    "UX",
    "firebase",
    "firebase authentication",
    "facebook",
    "google",
    "github",
    "twitter",
    "reset password",
    "user account",
    "angular fire",
    "firebaseui"
  ],
  "bugs": {
    "url": "https://github.com/AnthonyNahas/ionic-auth-firebaseui/issues"
  },
  "private": true,
  "scripts": {
    "lint": " gulp lint",
    "test": " gulp test",
    "test:ci": " gulp test:ci",
    "build": "gulp build",
    "demo": " gulp build:demo",
    "check-travis": " travis-status --repo=AnthonyNahas/ionic-auth-firebaseui --branch=master --fail-pending || echo 'Not yet passing'"
  },
  "dependencies": {
    "@angular/common": "^7.2.7",
    "@angular/core": "^7.2.7",
    "@angular/fire": "^5.1.1",
    "@angular/forms": "latest",
    "@ionic/angular": "^4.0.2",
    "firebase": "^5.8.4"
  },
  "devDependencies": {
    "@angular/animations": "^7.2.7",
    "@angular/compiler": "^7.2.7",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/platform-browser": "^7.2.7",
    "@angular/platform-browser-dynamic": "^7.2.7",
    "@angular/platform-server": "^7.2.7",
    "@compodoc/gulp-compodoc": "0.0.8",
    "@types/jest": "22.2.3",
    "@types/lodash": "4.14.92",
    "@types/node": "8.0.44",
    "angular-cli-ghpages": "0.5.2",
    "angular2-template-loader": "0.6.2",
    "ansi-colors": "2.0.1",
    "autoprefixer": "8.4.1",
    "awesome-typescript-loader": "5.0.0",
    "codecov": "^3.2.0",
    "codelyzer": "4.2.1",
    "commitplease": "3.2.0",
    "conventional-github-releaser": "2.0.2",
    "core-js": "2.5.5",
    "css-loader": "0.28.11",
    "cssnano": "3.10.0",
    "del": "3.0.0",
    "fancy-log": "1.3.2",
    "greenkeeper-lockfile": "1.14.0",
    "gulp": "3.9.1",
    "gulp-bump": "3.1.1",
    "gulp-conventional-changelog": "1.1.24",
    "gulp-coveralls": "0.1.4",
    "gulp-file": "0.4.0",
    "gulp-git": "2.6.0",
    "gulp-hub": "0.8.0",
    "gulp-inline-ng2-template": "4.1.0",
    "gulp-tslint": "8.1.3",
    "html-loader": "0.5.5",
    "jest": "22.4.3",
    "jest-cli": "^24.1.0",
    "jest-preset-angular": "5.2.2",
    "lodash": "4.17.10",
    "node-sass": "4.9.0",
    "postcss": "6.0.22",
    "postcss-strip-inline-comments": "0.1.5",
    "pump": "3.0.0",
    "raw-loader": "0.5.1",
    "rollup": "0.58.2",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-uglify": "3.0.0",
    "run-sequence": "2.2.1",
    "rxjs": "^6.4.0",
    "sass-loader": "7.0.1",
    "source-map-loader": "0.2.3",
    "to-string-loader": "1.1.5",
    "travis-status": "2.0.0",
    "tsickle": "^0.34.3",
    "tslint": "5.7.0",
    "typescript": "3.2.4",
    "yargs": "11.0.0",
    "zone.js": "0.8.26"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/src/"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/config",
      "/dist/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupJest.ts"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/core",
      "@angular/common",
      "@angular/compiler",
      "@angular/platform-server",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/compiler-cli",
      "zone.js",
      "rxjs",
      "tslint",
      "gulp-tslint",
      "typescript",
      "awesome-typescript-loader",
      "codelyzer",
      "@angular/animations",
      "firebase",
      "@angular/fire",
      "@types/jasmine"
    ]
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore",
      "revert",
      "demo"
    ],
    "scope": "\\S+.*"
  }
}
